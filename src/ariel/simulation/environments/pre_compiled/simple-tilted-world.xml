<mujoco model="Simple Tilted World">
  <compiler angle="radian"/>

  <option integrator="implicitfast"/>

  <visual>
    <global offwidth="1280" offheight="960"/>
  </visual>

  <asset>
    <texture type="2d" colorspace="auto" name="floor" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="600" height="600"/>
    <material name="floor" texture="floor" texuniform="true" texrepeat="3 3"/>
  </asset>

  <worldbody>
    <camera name="ortho-cam" pos="-5 0 5" quat="0.653281 0.270598 -0.270598 -0.653281" orthographic="true" fovy="5"/>
    <light name="light" pos="0 0 1" dir="0 0 -1" type="directional" castshadow="false"/>
    <body name="floor">
      <geom name="floor" size="5 5 0.5" quat="0.991445 0.130526 0 0" type="plane" material="floor"/>
    </body>
  </worldbody>
</mujoco>
